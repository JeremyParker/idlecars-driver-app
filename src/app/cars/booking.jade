a(ui-sref="carsShow({carId: car.id, car: car})") cancel
h2 Request for {{car.year}} {{car.make}} {{car.model}}

form(ng-submit='')

  div
    label(for='first_name') First Name
    input(id='first_name', ng-model='booking.first_name')

  div
    label(for='last_name') Last Name
    input(id='last_name', ng-model='booking.last_name')

  div
    label(for='email') Email Address
    input(id='email', ng-model='booking.email')

  div
    label(for='phone') Phone Number
    input(id='phone', ng-model='booking.phone')

  input(type='submit')

div {{booking}}
