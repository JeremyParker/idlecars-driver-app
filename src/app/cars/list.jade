h1(hidden) Rent cars for rideshare

.listing-filters
  .box(ng-click="didFilterCost('cheap')", ng-class="{active: costFilter.cheap}") < $50
  .box(ng-click="didFilterCost('medium')", ng-class="{active: costFilter.medium}") $50 - 80
  .box(ng-click="didFilterCost('pricey')", ng-class="{active: costFilter.pricey}") $80+

.listing-wrapper(save-scroll)

  a.car(ng-repeat="car in cars", ui-sref=".detail({carId: car.id, car: car})")
    car-image
    .info
      h3 {{car.name}}
      p {{car.listing_features}}
